(examples-for map-hash
  ("iterates over key-value pairs"
   (map-hash (fn (k v) "~k is for ~v")
             { a 1 b 2 c 3 })
   ("a is for 1" "b is for 2" "c is for 3"))

  ("sorts keys before iterating"
   (map-hash (fn (k v) "~k is for ~v")
             { "aa" 1 "bbb" 2 "c" 3 }
             (fn (keys) (sort-by len keys)))
   ("c is for 3" "aa is for 1" "bbb is for 2" )))
