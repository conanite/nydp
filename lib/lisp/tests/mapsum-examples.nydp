(examples-for mapsum
  ("maps a function over a list and sums the result"
   (mapsum car '((2 3) (5 7) (11 13)))
   18)

  ("extracts an attribute and sums the result"
   (mapsum &age (list { name "a" age 10 }
                      { name "b" age  7 }
                      { name "c" age 12 }))
   29))
