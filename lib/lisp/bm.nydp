(def bm-pythag ()
  (for i 1 100
    (for j 1 100
      (sqrt (+ (* i i) (* j j))))))

(def bmf (f n)
  (for b 1 n (f)))

(def bm (f n)
  (let time (millisecs)
    (bmf f n)
    (let elapsed (- (millisecs) time)
      (p "took: ~elapsed ms")
      (p "~n iterations, ~(/ elapsed n) ms per iteration")))
  nil)

(def rbs ()
  (bm bm-pythag 20))
